{
  "name": "today",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer-stylus": "^0.13.0",
    "concurrently": "^3.3.0",
    "react-scripts": "0.9.0",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.17.1",
    "object-hash": "^1.1.7",
    "re-base": "^2.6.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/styles.styl -o ./src/css/styles.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/styles.styl -o ./src/css/styles.css",
    "watch": "concurrently --names 'webpack, stylus' --prefix name 'npm run start' 'npm run styles:watch'"
  },
  "eslintConfig": {
    "extends": "node_modules/react-scripts/config/eslint.js"
  }
}
